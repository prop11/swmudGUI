		chatconsoles[tabname]:setColor( 25,25,25, 0)
		chatconsoles[tabname]:setFontSize(10)
		chatconsoles[tabname]:setWrap(globals.chatWrap)
		chatconsoles[tabname]:hide()
	end

	chatconsoles.chatAll:show()
	chatlabels.buttonAll:setColor( 50,50,50, 255 )
	globals.chatCurrent = &quot;chatAll&quot;
	setBorderTop(globals.chatHeight)
