	guiChatMain:move( globals.chatPosX .. &quot;px&quot;, globals.chatPosY .. &quot;px&quot; )
	guiChatMain:resize( globals.chatWidth + ( 2 * globals.chatTabWidth ) ..  &quot;px&quot;, globals.chatHeight .. &quot;px&quot;)

	local tabs = { &quot;All&quot;, &quot;Tells&quot;, &quot;City&quot;, &quot;House&quot;, &quot;Family&quot;, &quot;Pisces&quot;, &quot;Fight&quot;, &quot;Party&quot;, &quot;Mudlet&quot;, &quot;League&quot;, &quot;Shouts&quot;, &quot;Mariners&quot;, &quot;HNT&quot;, &quot;Teach&quot; }

	chatlabels = chatlabels or {}

	guiChatLabels = guiChatLabels or Geyser.Container:new({ name = &quot;guiChatLabels&quot; }, guiChatMain )
	guiChatLabels:move( globals.chatPosX .. &quot;px&quot;, globals.chatPosY .. &quot;px&quot;)
	guiChatLabels:resize( globals.chatTabWidth * 2 ..  &quot;px&quot;, globals.chatHeight .. &quot;px&quot;)

	for i,v in ipairs(tabs) do
		local tabname, posX, posY

		tabname = &quot;button&quot; .. string.title(v)
