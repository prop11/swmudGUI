		str = string.gsub(channel, &quot;chat&quot;, &quot;button&quot;)
		chatlabels[str]:setColor( 50,50,50, 255 )
		globals.chatCurrent = channel
		setBorderTop( globals.chatHeight )
	elseif globals.chatCurrent == channel then
		chatconsoles[channel]:hide()
		str = string.gsub(globals.chatCurrent, &quot;chat&quot;, &quot;button&quot;)
		chatlabels[str]:setColor( 25,25,25, 0 )
		globals.chatCurrent = &quot;none&quot;
		setBorderTop( 0 )
	else
		chatconsoles[globals.chatCurrent]:hide()
		str = string.gsub(globals.chatCurrent, &quot;chat&quot;, &quot;button&quot;)
		chatlabels[str]:setColor( 25,25,25, 0 )

		chatconsoles[channel]:show()
		str = string.gsub(channel, &quot;chat&quot;, &quot;button&quot;)
		chatlabels[str]:setColor( 50,50,50, 255 )
		globals.chatCurrent = channel
	end
