		chatlabels[tabname]:setClickCallback(&quot;chatSwapTab&quot;, &quot;chat&quot; .. v )
	end

	chatconsoles = chatconsoles or {}

	guiChatConsoles = guiChatConsoles or Geyser.Container:new({ name = &quot;guiChatLabels&quot; }, guiChatMain )
